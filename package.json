{
  "name": "react-native-solarview-components",
  "version": "1.0.1",
  "description": "SolarView",
  "license": "ISC",
  "main": "lib/commonjs/index.js",
  "react-native": "lib/module/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "lib",
    "!android/build",
    "!ios/build",
    "!lib/typescript/Assets",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/__snapshots__",
    "!**/Sandbox",
    "!**/*.test.js",
    "!**/*.test.js.map"
  ],
  "sideEffects": false,
  "scripts": {
    "start": "expo start",
    "prepare": "yarn build && node ./scripts/generate-mappings.js",
    "build": "bob build && rimraf lib/**/*.map",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "yarn update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "storybook dev -p 6006",
    "commit": "git-cz",
    "test:generate-output": "jest -u --json --outputFile=.jest-test-results.json || true",
    "test": "jest --verbose --coverage --updateSnapshot --watchAll=false --maxWorkers=50%",
    "test:export": "jest -u --coverage --json --outputFile=.jest-test-results.json || true",
    "testdev": "jest --watch ",
    "testdevc": "jest --watch --coverage ",
    "testview": "npx live-server reports/lcov-report ",
    "prebuild-storybook": "yarn test:generate-output",
    "build-storybook": "build-storybook -c .storybook -s .storybook/assets,./assets -o public_storybook",
    "storybook:screenshot": "rm -rf ./__screenshots__ && yarn storycap http://localhost:9001 --viewport '414x736' --serverTimeout '200000'",
    "tsc": "rimraf dist && tsc",
    "prettier": "prettier $PWD/\"src/**/*.ts*\" --write",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "lint:formater": "yarn format &&  yarn lint",
    "lint:report": "eslint --quiet --ext .js,.jsx,.ts,.tsx src -f json -o test/coverage/eslint-report.json",
    "release": "release-it"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged && git add ."
    }
  },
  "lint-staged": {
    "*.ts*": [
      "yarn prepare",
      "prettier --write",
      "eslint --fix",
      "jest --passWithNoTests",
      "git add"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.5.2",
    "@react-native-community/slider": "4.2.4",
    "@storybook/addon-a11y": "^7.0.12",
    "@storybook/addon-actions": "~7.0",
    "@storybook/addon-controls": "~7.0",
    "@storybook/addon-jest": "^7.0.12",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-knobs": "^6.5.3",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "@testing-library/jest-dom": "5.10.0",
    "@testing-library/jest-native": "4.0.2",
    "@testing-library/react-native": "7.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "^48.0.17",
    "expo-app-loading": "~2.1.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-modules-core": "^1.2.7",
    "expo-status-bar": "~1.4.2",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.0",
    "jest-styled-components": "7.0.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "~0.18.7",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@release-it/conventional-changelog": "1.1.0",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@storybook/addon-essentials": "~7.0",
    "@storybook/addon-links": "~7.0",
    "@storybook/addon-mdx-gfm": "^7.0.12",
    "@storybook/addon-react-native-web": "^0.0.20",
    "@storybook/react": "~7.0",
    "@storybook/react-webpack5": "^7.0.12",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "^27.4.1",
    "@types/prettier": "2.0.0",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.59.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-react-native-web": "^0.17.5",
    "babel-plugin-root-import": "^6.6.0",
    "babel-plugin-styled-components": "^2.1.3",
    "babel-preset-expo": "~9.2.1",
    "babel-types": "^6.26.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.6.12",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.2.5",
    "jest-expo": "~41.0.0",
    "jest-sonar": "^0.2.16",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^10.2.11",
    "prettier": "2.0.5",
    "prettier-eslint-cli": "5.0.0",
    "react-native-builder-bob": "^0.18.1",
    "react-native-dotenv": "^3.3.1",
    "release-it": "13.4.0",
    "storybook": "^7.0.12",
    "typescript": "^4.6.3"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "copyFlow": true
        }
      ],
      "module",
      "typescript"
    ]
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "private": true,
  "repository": "git@github.com:T8-Infinity/ds-solarview.git",
  "author": "devdanalmeida@gmail.com <devdanalmeida@gmail.com>"
}
