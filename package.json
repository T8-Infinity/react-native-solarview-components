{
  "name": "react-native-solarview-components",
  "version": "1.0.0",
  "description": "SolarView components for React Native",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "yarn update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "commit": "git-cz",
    "test:generate-output": "jest -u --json --outputFile=.jest-test-results.json || true",
    "test": "jest --verbose --coverage --updateSnapshot --watchAll=false --maxWorkers=50%",
    "test:export": "jest -u --coverage --json --outputFile=.jest-test-results.json || true",
    "testdev": "jest --watch ",
    "testdevc": "jest --watch --coverage ",
    "testview": "npx live-server reports/lcov-report "
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint --fix",
      "jest --passWithNoTests",
      "git add"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.2",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/datetimepicker": "6.5.2",
    "@react-native-community/slider": "4.2.4",
    "@storybook/addon-a11y": "^7.0.12",
    "@storybook/addon-actions": "~7.0",
    "@storybook/addon-controls": "~7.0",
    "@storybook/addon-jest": "^7.0.12",
    "@storybook/addon-knobs": "^7.0.2",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-knobs": "^6.5.3",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "@testing-library/jest-dom": "5.10.0",
    "@testing-library/jest-native": "4.0.2",
    "@testing-library/react-native": "7.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "expo": "^47.0.0",
    "expo-app-loading": "~2.1.1",
    "expo-linear-gradient": "~12.0.1",
    "expo-status-bar": "~1.4.2",
    "jest": "^26.6.3",
    "jest-expo": "^47.0.0",
    "jest-styled-components": "7.0.3",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-reanimated": "~2.12.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "~0.18.7",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-essentials": "~7.0",
    "@storybook/addon-links": "~7.0",
    "@storybook/addon-mdx-gfm": "^7.0.12",
    "@storybook/addon-react-native-web": "^0.0.20",
    "@storybook/react": "~7.0",
    "@storybook/react-webpack5": "^7.0.12",
    "@types/jest": "^27.4.1",
    "@types/react": "~18.0.24",
    "@types/react-native": "~0.70.6",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-react-native-web": "^0.17.5",
    "babel-plugin-root-import": "^6.6.0",
    "babel-preset-expo": "~9.2.1",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "eslint-plugin-storybook": "^0.6.12",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.2.5",
    "jest-expo": "~41.0.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^10.2.11",
    "react-native-dotenv": "^3.3.1",
    "storybook": "^7.0.12",
    "typescript": "^4.6.3"
  },
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/changelog",
    "@semantic-release/github",
    "@semantic-release/npm",
    "@semantic-release/git"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "private": true
}
