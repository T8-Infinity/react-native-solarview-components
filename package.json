{
  "name": "ds-toAqui",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "prestart": "yarn update-stories",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "commit": "git-cz",
    "test": "jest --watchAll=false",
    "test:export": "jest -u --coverage --json --outputFile=.jest-test-results.json || true"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.tsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@expo-google-fonts/poppins": "^0.2.2",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "4.0.0",
    "@react-native-community/slider": "4.1.12",
    "@storybook/addon-a11y": "^6.5.15",
    "@storybook/addon-actions": "~6.5",
    "@storybook/addon-controls": "~6.5",
    "@storybook/addon-jest": "^6.5.15",
    "@storybook/addon-knobs": "^6.4.0",
    "@storybook/addon-ondevice-actions": "next",
    "@storybook/addon-ondevice-backgrounds": "next",
    "@storybook/addon-ondevice-controls": "next",
    "@storybook/addon-ondevice-notes": "next",
    "@storybook/react-native": "next",
    "@testing-library/jest-dom": "5.10.0",
    "@testing-library/jest-native": "4.0.2",
    "@testing-library/react-native": "7.2.0",
    "expo": "~44.0.0",
    "expo-app-loading": "^1.3.0",
    "expo-linear-gradient": "~11.0.3",
    "expo-status-bar": "~1.2.0",
    "jest": "^26.6.3",
    "jest-expo": "^44.0.1",
    "jest-styled-components": "7.0.3",
    "react": "17.0.1",
    "react-dom": "^17.0.2",
    "react-native": "0.64.3",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-iphone-x-helper": "^1.3.1",
    "react-native-reanimated": "^2.6.0",
    "react-native-responsive-fontsize": "^0.5.1",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-web": "^0.17.5",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-essentials": "~6.5",
    "@storybook/addon-links": "~6.5",
    "@storybook/addon-react-native-web": "^0.0.19",
    "@storybook/react": "~6.5",
    "@types/jest": "^27.4.1",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-react-native-web": "^0.17.5",
    "babel-plugin-root-import": "^6.6.0",
    "babel-preset-expo": "8.3.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.6",
    "eslint-plugin-storybook": "^0.6.10",
    "git-commit-msg-linter": "^3.2.8",
    "husky": "^4.2.5",
    "jest-expo": "~41.0.0",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^10.2.11",
    "react-native-dotenv": "^3.3.1",
    "typescript": "~4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "resolutions": {
    "styled-components": "^5"
  },
  "private": true
}
